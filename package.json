  "scripts": {
    "analyze": "BUNDLE_ANALYZE=true razzle build",
    "build": "razzle build",
    "postinstall": "!(test -n \"$POST_INSTALL\") || (pwd && razzle build)",
    "start": "NODE_ENV=production node build/server.js",
    "start:dev": "razzle start",
    "test": "razzle test --env=jsdom",
    "test:ci": "razzle test --env=jsdom --verbose --runInBand --watchAll=false"
